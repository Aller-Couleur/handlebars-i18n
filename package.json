{
  "name": "handlebars-i18n",
  "version": "1.1.3",
  "description": "handlebars-i18n adds internationlization to handlebars.js using i18next and Intl",
  "main": "dist/handlebars-i18n.js",
  "scripts": {
    "test": "nyc mocha",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwalzel/handlebars-i18n.git"
  },
  "keywords": [
    "handlebars-i18n",
    "Internationalization",
    "Localization",
    "Globalization",
    "Translation",
    "Date Formatting",
    "Currency Formatting",
    "Number Formatting",
    "Handlebars",
    "Handlebars.js",
    "i18next",
    "i18n",
    "Intl",
    "Helper",
    "handlebars-i18next"
  ],
  "author": "Florian Walzel",
  "license": "MIT",
  "homepage": "https://github.com/fwalzel/handlebars-i18n.git#readme",
  "peerDependencies": {
    "handlebars": "^4.7.6",
    "i18next": "^19.8.4 || ^20.0.0",
    "intl": "^1.2.5"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "coveralls": "3.1.0",
    "gulp": "4.0.2",
    "gulp-rename": "2.0.0",
    "gulp-uglify": "3.0.2",
    "gulp-uglify-es": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "8.3.2",
    "nyc": "15.1.0"
  },
  "dependencies": {
    "handlebars": "^4.7.6",
    "i18next": "^19.8.4",
    "intl": "^1.2.5"
  }
}
